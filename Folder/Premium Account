
class PremiumAccount extends RegularAccount {
    public PremiumAccount(String name, double initialDeposit) {
        super(name, initialDeposit);
    }

    @Override
    public void withdraw(double amount) throws Exception {
        if (getbal() - amount < 0) {
            throw new Exception("Insufficient balance!");
        }
        if (getbal() - amount < MIN_BALANCE) {
            System.out.println("Warning: Balance is below minimum limit!");
        }
        setBal(getbal() - amount);
        System.out.println("Withdrawal successful. New Balance: â‚¹" + getbal());
    }


}
